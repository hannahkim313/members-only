<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%- title %></title>
  </head>
  <body>
    <h1>Members Only</h1>
    <a href="/auth/sign-up">Sign up</a>
    <a href="/auth/login">Login</a>
    <h2>Messages</h2>
    <% messages.forEach((message) => { %>
    <article>
      <h3><%= message.title %></h3>
      <p><%= message.message %></p>
    </article>
    <% }) %>
  </body>
</html>
